#include<stdio.h>
float sumfloat(float a,float b);
char subschar(char a,char b);
float multiply(float a,float b);
float salary(float hours,float wage);
float percent(float number,float part);
int main()
{
float result_1 = 0;
char result_2 =0;
float result_3 =0;
float result_4 =0;
float result =0;
result_1 = sumfloat(4.58,7.89);
result_2 = subschar(45,27);
result_3 = multiply(3.47,8.97);
result_4 = salary(207.8,37);
result  = percent(345.278, 45.67);
printf("Result is %.2f\n", result_1);
printf("Result is %d \n", result_2);
printf("Result is %.2f \n", result_3);
printf("My salary is %.2f \n",result_4);
printf("This is your new number %f \n",result);
	return 0;
}
float sumfloat(float a,float b)
{
	return a+b;
}
char subschar(char a,char b)
{
	return a-b;
}
float multiply(float a,float b)
{
	return a*b;
}
float salary(float hours,float wage)
{
	return hours*wage;
}
float percent(float number,float part)
{
	return number*0.01*part;
}
